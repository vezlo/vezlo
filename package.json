{
  "name": "@vezlo/assistant-server",
  "version": "1.1.0",
  "description": "Production-ready AI Assistant Server with vector search, conversation management, and real-time communication",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "vezlo-server": "./bin/vezlo-server.js",
    "vezlo-setup": "./scripts/setup.js",
    "vezlo-validate": "./scripts/validate-db.js"
  },
  "files": [
    "dist/",
    "bin/",
    "scripts/",
    "database-schema.sql",
    "knexfile.ts",
    "env.example",
    "Dockerfile",
    "docker-compose.yml",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "vercel-build": "tsc",
    "start": "node dist/src/server.js",
    "prepublishOnly": "npm run build",
    "setup": "node scripts/setup.js",
    "validate": "node scripts/validate-db.js",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:status": "knex migrate:status",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "seed:make": "knex seed:make",
    "seed:run": "knex seed:run"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "socket.io": "^4.7.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "knex": "^3.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/uuid": "^9.0.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/pg": "^8.10.9",
    "@vercel/node": "^3.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "api",
    "typescript",
    "supabase",
    "vector-search",
    "embeddings",
    "openai",
    "chat",
    "conversation",
    "websocket",
    "real-time",
    "knowledge-base",
    "semantic-search"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vezlo/assistant-server.git"
  },
  "bugs": {
    "url": "https://github.com/vezlo/assistant-server/issues"
  },
  "homepage": "https://github.com/vezlo/assistant-server#readme",
  "author": "Vezlo <ashaheen@workhub.ai>",
