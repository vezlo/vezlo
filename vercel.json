{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/setup-page"
    },
    {
      "source": "/setup",
      "destination": "/api/setup-page"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
