{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "ENABLE_TYPESCRIPT_PLUGIN": "false"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
